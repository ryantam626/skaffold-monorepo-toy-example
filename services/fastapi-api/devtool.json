{
  "pod-pattern": "dev-fastapi-api-.*",
  "skaffold": {
    "module": "api",
    "reverseFileSync": [
      {
        "src": "/app",
        "dst": "services/fastapi-api/app"
      },
      {
        "src": "/pyproject.toml",
        "dst": "services/fastapi-api/pyproject.toml"
      },
      {
        "src": "/poetry.lock",
        "dst": "services/fastapi-api/poetry.lock"
      }
    ]
  }
}

